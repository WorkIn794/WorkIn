{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@styles/*": ["src/styles/*"],
      "@icons/*": ["src/icons/*"],
      "@svelteComponents/*": ["src/components/svelte/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}